# Local ISO Repository
# Generated by Ansible

# For RHEL 8/9 - BaseOS
[local-iso-baseos]
name=Local ISO Repository - BaseOS
baseurl=http://{{ local_repo_server_ip }}:{{ yum_repo_web_port }}/iso-repo/BaseOS
enabled=1
gpgcheck=0
priority=1

# For RHEL 8/9 - AppStream
[local-iso-appstream]
